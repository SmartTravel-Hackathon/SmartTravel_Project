<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="16dp"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textNombre3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button7"
        android:layout_marginTop="103dp"
        android:text="Filtrado"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/text_pantallaprueba"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerInParent="true"
        android:text="Pantalla Prueba"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textNombre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/text_pantallaprueba"
        android:text="Jonathan:"
        android:textSize="20sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button_inicio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textNombre"
        android:text="Pantalla Inicio"
        android:backgroundTint="#23B894"/>

    <Button
        android:id="@+id/button_login"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textNombre"
        android:layout_toRightOf="@+id/button_inicio"
        android:text="Pantalla Login"
        android:backgroundTint="#23B894"/>

    <Button
        android:id="@+id/button_registeragency"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textNombre"
        android:layout_toRightOf="@+id/button_login"
        android:text="Pantalla Registro A"
        android:backgroundTint="#23B894"/>

    <Button
        android:id="@+id/button_registertourist"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button_inicio"
        android:text="Pantalla Registro T"
        android:backgroundTint="#23B894"/>

    <Button
        android:id="@+id/button_carga"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button_inicio"
        android:layout_toRightOf="@+id/button_registertourist"
        android:text="1 Pantalla Carga"
        android:backgroundTint="#23B894"/>

    <Button
        android:id="@+id/button_Reserva_Carro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button_registertourist"
        android:text="Reserva Carro"
        android:backgroundTint="#23B894"/>

    <Button
        android:id="@+id/button_Datos_del_viajero"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button_carga"
        android:layout_centerInParent="true"
        android:layout_toRightOf="@+id/button_Reserva_Carro"
        android:backgroundTint="#23B894"
        android:text="Datos Viajero" />

    <Button
        android:id="@+id/button_Resumen_Reserva"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button_carga"
        android:layout_toRightOf="@+id/button_Datos_del_viajero"
        android:text="Resumen Reserva"
        android:backgroundTint="#23B894"/>

    <Button
        android:id="@+id/button_Pagar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button_Reserva_Carro"
        android:text="PAGAR"
        android:backgroundTint="#23B894"/>

    <Button
        android:id="@+id/button_Carga2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button_Datos_del_viajero"
        android:layout_toRightOf="@+id/button_Pagar"
        android:text="2 Pantalla carga"
        android:backgroundTint="#23B894"/>

    <TextView
        android:id="@+id/textNombre2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button_Carga2"
        android:text="Felipe:"
        android:textSize="20sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textNombre2"
        android:text="Pasar A Filtro"
        android:backgroundTint="#23B894"/>

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/textNombre2"
        android:layout_toRightOf="@+id/button"
        android:text="Pasar A Ordenar"
        android:backgroundTint="#23B894"/>

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/button"
        android:text="Pasar A Detalles Agencia"
        android:backgroundTint="#23B894"/>

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/button2"
        android:layout_centerInParent="true"
        android:layout_toRightOf="@+id/button3"
        android:backgroundTint="#23B894"
        android:text="Pasar A Reserva Hoteles" />

    <Button
        android:id="@+id/button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/button4"
        android:backgroundTint="#23B894"
        android:text="Pasar A Reserva Restaurantes" />

    <Button
        android:id="@+id/button6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/button4"
        android:layout_toRightOf="@+id/button5"
        android:text="DetallesPago"
        android:backgroundTint="#23B894"/>

    <Button
        android:id="@+id/button7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/button5"
        android:text="Notificaciones"
        android:backgroundTint="#23B894"/>

    <Button
        android:id="@+id/buttonPerfil"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/button5"
        android:layout_toRightOf="@+id/button7"
        android:text="Perfil"
        android:backgroundTint="#23B894"/>

    <Button
        android:id="@+id/buttonHome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/textNombre3"
        android:layout_marginTop="-102dp"
        android:backgroundTint="#23B894"
        android:text="Home" />

    <Button
        android:id="@+id/buttonFiltrados"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/FiltradosText"
        android:layout_marginTop="107dp"
        android:backgroundTint="#23B894"
        android:text="FiltrosImplementados" />

    <TextView
        android:id="@+id/FiltradosText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button7"
        android:text="Sara:"
        android:textSize="20sp"
        android:textStyle="bold" />



</RelativeLayout>